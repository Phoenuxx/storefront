{
  "name": "storefront",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd backend node server.js",
    "start:dev": "concurrently \"nodemon ./backend/server.js --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phoenuxx/storefront.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Phoenuxx/storefront/issues"
  },
  "homepage": "https://github.com/Phoenuxx/storefront#readme",
  "engines": {
    "node": "10.15.1"
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "if-env": "^1.0.4"
  }
}
